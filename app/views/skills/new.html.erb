<!-- Wraps the entire page -->
<div class="row main-row">
  <div class="medium-10 columns medium-centered">

    <ul class="tabs" data-tab data-options="scroll_to_content: false">
      <li class="tab-title"><a href="#share">Share a skill</a></li>
      <li class="tab-title"><a href="#user-listings"><%= current_user.first_name %>'s Inbox</a></li>
    </ul>

    <div class="tabs-content">

      <div class="content" id="share-content">
        <div class="row">
          <div class="medium-12 columns">
            <li><%= image_tag("/assets/running-pair.jpg") %></li>
          </div>
        </div>
        <div class="row top-spacer" data-equalizer>
        </div>
        <%= render "form" %>

      </div>

      <div class="content" id="user-listings-content">
       

        
        <%= render "share" %>
      </div>
    </div>
  </div>
</div>